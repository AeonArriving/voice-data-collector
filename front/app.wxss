/**app.wxss**/
@import "./utils/iconfont.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.main-container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.title-container {
  padding-top: 120rpx;
  padding-bottom: 120rpx;
  height: 180rpx;
  background-color: rgb(81, 190, 241)	;
  width: 100%;
  display: flex;
  flex-direction: column;
  padding-left: 50px;
}

.title-inner-container {
   display: flex;
   flex-direction: column;
   align-items: flex-start;
   font-family: fantasy;
   font-weight: bold;
   color: white;
}

